pipeline {
    agent any
    stages {
        stage ('instal playwright') {
            steps {
             bat  "npm install"
             bat  "npm install @cucumber/cucumber@7.3.2"
               
            }
        }
        stage ('run test') {
            steps {
               
             bat "npm run e2e"
                
            }
        }
           stage ('export report') {
            steps {
                
              bat  "node report_html.js"
               
            }
        }
    }
}