pipeline {
    agent any
    stages {
        stage ('instal playwright') {
            steps {
                sh ''' 
                npm install 
                '''
            }
        }
        stage ('run test') {
            steps {
                sh ''' 
                npm run e2e
                '''
            }
        }
           stage ('export report') {
            steps {
                sh ''' 
                node report_html.js
                '''
            }
        }
    }
}